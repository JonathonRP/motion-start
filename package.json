{
  "name": "motion-start",
  "version": "0.1.0",
  "description": "Svelte animation library inspired by the React library framer-motion.",
  "license": "MIT",
  "author": {
    "name": "JonathonRP",
    "url": "https://github.com/JonathonRP"
  },
  "repository": "github:JonathonRP/motion-start",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src/*.js": {
      "types": "./dist/*.d.ts",
      "svelte": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./src/*.svelte": {
      "types": "./dist/*.d.ts",
      "svelte": "./dist/*.svelte",
      "default": "./dist/*.svelte"
    }
  },
  "files": ["dist", "!dist/**/*.test.*", "!dist/**/*.spec.*"],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "sv": "bunx sv",
    "dev": "vite dev",
    "build": "vite build && bun --bun package",
    "package": "svelte-kit sync && svelte-package --input ./src/lib/motion-start && publint --strict",
    "preview": "vite preview",
    "check": "svelte-kit sync && sv check --tsconfig ./tsconfig.json",
    "lint": "biome check",
    "cypress": "cypress open",
    "test": "vitest",
    "test:types": "vitest --typecheck.only",
    "test:ui": "vitest --ui",
    "test:types:ui": "vitest --ui",
    "release": "bun --bun run build && bun --bun changeset publish"
  },
  "dependencies": {
    "framer-motion": "^12.15.0",
    "motion-dom": "^12.15.0",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.4",
    "@emotion/is-prop-valid": "^1.3.1",
    "@lucide/svelte": "^0.503.0",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@tsconfig/svelte": "^5.0.4",
    "@types/cypress": "^1.1.6",
    "@types/node": "^20.17.54",
    "@vitest/ui": "^2.1.9",
    "add": "^2.0.6",
    "bits-ui": "^1.8.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csstype": "^3.1.3",
    "cypress": "^14.4.0",
    "cypress-real-events": "^1.14.0",
    "mode-watcher": "^0.5.1",
    "publint": "^0.3.12",
    "runed": "^0.28.0",
    "sv": "^0.8.7",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.2.1",
    "svelte-render-scan": "^1.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.8",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "svelte": "^5.33.10"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=20"
  },
  "sideEffects": false,
  "keywords": ["svelte animation", "svelte", "animation", "gestures", "drag", "spring", "popmotion", "framer-motion"],
  "trustedDependencies": ["@biomejs/biome", "@sveltejs/kit", "@tailwindcss/oxide", "cypress", "esbuild", "svelte-preprocess"]
}
