{
	"name": "motion-start",
	"version": "0.1.0",
	"description": "Svelte animation library inspired by the React library framer-motion.",
	"license": "MIT",
	"author": {
		"name": "JonathonRP",
		"url": "https://github.com/JonathonRP"
	},
	"repository": {
		"type": "git",
		"ur": "https://github.com/JonathonRP/motion-start.git"
	},
	"type": "module",
	"files": ["dist"],
	"main": "./dist/index.js",
	"bin": "./dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": "./dist/index.js",
	"scripts": {
		"sv": "bunx sv",
		"dev": "vite dev",
		"build": "vite build && bun --bun package",
		"package": "svelte-kit sync && svelte-package --input ./src/lib/motion-start && publint --strict",
		"preview": "vite preview",
		"check": "svelte-kit sync && sv check --tsconfig ./tsconfig.json",
		"lint": "biome check",
		"cypress": "cypress open",
		"test": "vitest",
		"test:types": "vitest --typecheck.only",
		"test:ui": "vitest --ui",
		"test:types:ui": "vitest --ui",
		"release": "bun --bun run build && bun --bun changeset publish"
	},
	"dependencies": {
		"nanoid": "^5.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.28.1",
		"@emotion/is-prop-valid": "^1.3.1",
		"@lucide/svelte": "^0.487.0",
		"@sveltejs/kit": "^2.20.5",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.3",
		"@tsconfig/svelte": "^5.0.4",
		"@types/cypress": "^1.1.6",
		"@types/node": "^20.17.30",
		"@vitest/ui": "^2.1.9",
		"add": "^2.0.6",
		"bits-ui": "^1.3.19",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"csstype": "^3.1.3",
		"cypress": "^14.3.0",
		"cypress-real-events": "^1.14.0",
		"mode-watcher": "^0.5.1",
		"publint": "^0.3.11",
		"runed": "^0.25.0",
		"sv": "^0.8.1",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.1.6",
		"svelte-render-scan": "^1.1.0",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.3",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vitest": "^2.1.9"
	},
	"peerDependencies": {
		"svelte": "^5.26.2"
	},
	"engines": {
		"bun": ">=1.0.0",
		"node": ">=20"
	},
	"sideEffects": false,
	"keywords": ["svelte animation", "svelte", "animation", "gestures", "drag", "spring", "popmotion", "framer-motion"],
	"trustedDependencies": ["@biomejs/biome", "@sveltejs/kit", "esbuild", "svelte-preprocess"]
}
